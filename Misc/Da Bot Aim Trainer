if not game.PlaceId == 109180248972375 then return end

local l = game:GetService("Players").LocalPlayer

local function r(c)
    local bd = c and c:FindFirstChild("BloxDog", true)
    if bd then
        pcall(bd.Destroy, bd)
    end
end

if l.Character then
    r(l.Character)
end

l.CharacterAdded:Connect(function(c)
    task.wait(2)
    r(c)
end)
